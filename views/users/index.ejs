<h2>Search Users</h2>
<form action="/users" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<form action="/users" method="GET">
    <label>Email</label>
    <input type="text" name="email">
    <button type="submit">Search</button>
</form>
<br>
<% users.forEach(user => { %>
     <br>
     <div><%= user.name %></div>
     <div><%= user.email %></div>
<% }) %> 